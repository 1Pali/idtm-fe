<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core"
>
    <Button
        text="{i18n>EditEntityTableButton}"
        tooltip="{i18n>EditEntityTableButtonToolTip}"
        visible="{= ${ui>/editMode} === false}"
        type="Emphasized"
        press=".onEditPress"
    />

    <Button
        text="{i18n>DeleteEntityTableButton}"
        tooltip="{i18n>DeleteEntityTableButtonToolTip}"
        type="Transparent"
        press=".onDeletePress"
    />
</core:FragmentDefinition>
